import java.util.ArrayList;

public class ArrayListExample {
    public static void main(String[] args) {
        
        ArrayList<String> foodcrops = new ArrayList<>();
        foodcrops.add("cassava");
        foodcrops.add("matooke");
        foodcrops.add("rice");

        System.out.println("First foodcrop: " + foodcrops.get(0));
        System.out.println("Last foodcrop: " + foodcrops.get(foodcrops.size() - 1));
    }
}
