import java.util.ArrayList;

public class ArrayListExample {
    public static void main(String[] args) {
        
        ArrayList<String> foodcrops = new ArrayList<>();
        foodcrops.add("cassava");
        foodcrops.add("matooke");
        foodcrops.add("rice");

        foodcrops.remove(1); 

        System.out.println(foodcrops);
    }
}
